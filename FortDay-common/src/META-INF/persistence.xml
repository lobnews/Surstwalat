<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FortDayDB" transaction-type="JTA">
    <jta-data-source>java:app/jdbc/FortDayDB</jta-data-source>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Account</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Action</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.ActionResult</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.ActionType</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Dice</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Field</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Game</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.HealthItem</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Item</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Player</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Playground</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.RollActionResult</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.Token</class>
    <class>de.fh_dortmund.inf.cw.surstwalat.common.model.DamageZone</class>
    <properties>
      <property name="eclipselink.target-database" value="MySQL"/>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
      <property name="eclipselink.ddl-generation.output-mode" value="database"/>
      <property name="eclipselink.logging.level" value="INFO"/>
    </properties>
  </persistence-unit>
</persistence>
